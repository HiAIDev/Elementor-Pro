{
  "name": "element-pro",
  "version": "0.0.1",
  "description": "element-pro components",
  "main": "lib/index.ssr.js",
  "browser": "lib/index.esm.js",
  "module": "lib/index.esm.js",
  "unpkg": "lib/index.min.js",
  "types": "types/src/index.d.ts",
  "style": "lib/index.min.css",
  "scripts": {
    "dev": "vite",
    "watch": "yarn build:es -w",
    "build": "yarn build:ts && yarn build:types && yarn build:style",
    "build:ts": "rimraf lib && cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "build:types": "rimraf types && tsc -d",
    "build:style": "postcss lib/index.css -o lib/index.min.css",
    "build:example": "vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tolking/element-pro.git"
  },
  "keywords": [
    "element-pro",
    "admin",
    "element-plus",
    "components"
  ],
  "author": "<tolking qw13131wang@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tolking/element-pro/issues"
  },
  "homepage": "https://github.com/tolking/element-pro#readme",
  "dependencies": {
    "element-plus": "^1.0.1-beta.5",
    "vue": "^3.0.4",
    "vue-router": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/compiler-sfc": "^3.0.4",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "minimist": "^1.2.5",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.1",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.34.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-css-only": "^3.0.0",
    "rollup-plugin-esbuild": "^2.6.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^4.1.3",
    "vite": "^1.0.0-rc.13",
    "vue-template-compiler": "^2.6.12"
  }
}
